package HinhHoc;

public class HinhTron extends Hinh{
    private float r;
    private double pi = 3.14;

	public HinhTron(float r){
        this.r = r;
    }

    public String toString(){
        return "Ban kinh " + this.r;
    }

    public double dienTich(){
    	double dt = this.r * this.r * pi;
        return dt;
    }
}
