package HinhHoc;
import java.util.ArrayList;

public class DanhSachHinh {
    private ArrayList<Hinh> listHinh;

    public DanhSachHinh(ArrayList<Hinh> listHinh) {
        this.listHinh = new ArrayList<Hinh>();
    }
    
    public DanhSachHinh() {
    	
    }

    public void Them(Hinh listHinh){
        this.listHinh.add(listHinh);
    }

    public void InDanhSach(){
        for(Hinh Hinh : listHinh) {
            System.out.println(Hinh);
        }
    }
}
