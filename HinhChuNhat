package HinhHoc;

public class HinhChuNhat extends Hinh{
    public float cd;
    public float cr;

    public HinhChuNhat(float cd, float cr){
        this.cd = cd;
        this.cr = cr;
    }
    public String toString(){
        return "chieu dai: " + this.cd + " - chieu rong: " + this.cr;
    }
    public double dienTich(){
    	double dt = this.cd * this.cr;
    	return dt;
    }
}
